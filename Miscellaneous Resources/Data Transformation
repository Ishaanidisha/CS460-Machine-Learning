   "source": [
    "plt.figure(figsize=(12, 6))\n",
    "sns.histplot(df['Income'], kde=True, bins=30)\n",
    "plt.title('Original Distribution: Right-Skewed')\n",
    "plt.xlabel('Income')\n",
    "plt.ylabel('Frequency')\n",
    "plt.show()"
   ]
