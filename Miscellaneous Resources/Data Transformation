   "source": [
    "df['BoxCox_Income'], _ = boxcox(df['Income'] + 1)  # Adding 1 to handle zero values\n",
    "\n",
    "# Visualize the Box-Cox Transformed Distribution\n",
    "plt.figure(figsize=(12, 6))\n",
    "sns.histplot(df['BoxCox_Income'], kde=True, bins=30, color='green')\n",
    "plt.title('Box-Cox Transformed Distribution')\n",
    "plt.xlabel('Box-Cox(Income)')\n",
    "plt.ylabel('Frequency')\n",
    "plt.show()"
